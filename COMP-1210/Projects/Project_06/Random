         {
            index++;
         } 
         else 
         {
            break;  
         }
      }
      if (iceCreamConeObject.getLabel().compareToIgnoreCase(label) != 0)
      {
         return false;
      }
      iceCreamConeObject.setRadius(radius);
      iceCreamConeObject.setHeight(height);
      return true;
                        
                        
                        
      
